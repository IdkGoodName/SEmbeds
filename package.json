{
  "name": "sembeds",
  "version": "1.0.0",
  "description": "Allows you to send custom embeds",
  "main": "index.js",
  "scripts": {
    "rollup": "rollup -c rollup.config.js",
    "package": "tar -czf SEmbeds-ReGuilded-0.0.4.tar.gz SEmbeds && zip -r SEmbeds-ReGuilded-0.0.4.zip SEmbeds",
    "build": "cp ./src/index.js ./src/metadata.json ./src/README.md ./SEmbeds && npm run package",
    "rebuild": "rm -r SEmbeds && mkdir SEmbeds && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IdkGoodName/SEmbeds.git"
  },
  "keywords": [
    "embeds",
    "custom",
    "send",
    "messages",
    "content",
    "message",
    "guilded",
    "reguilded",
    "addon",
    "plugin",
    "extension"
  ],
  "author": "IdkGoodName",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/IdkGoodName/SEmbeds/issues"
  },
  "homepage": "https://github.com/IdkGoodName/SEmbeds#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "rollup": "^2.66.1"
  }
}
